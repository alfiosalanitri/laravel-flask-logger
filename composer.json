{
    "name": "alfiosalanitri/laravel-flask-logger",
    "description": "A log handler for Laravel that sends logs to a Flask server started with https://github.com/alfiosalanitri/flask-log-monitor",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alfio Salanitri"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "monolog/monolog": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AlfioSalanitri\\FlaskLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlfioSalanitri\\FlaskLogger\\FlaskLoggerServiceProvider"
            ]
        }
    },
    "homepage": "https://github.com/alfiosalanitri/laravel-flask-logger",
    "support": {
      "issues": "https://github.com/alfiosalanitri/laravel-flask-logger/issues",
      "source": "https://github.com/alfiosalanitri/laravel-flask-logger",
      "dependencies": {
        "flask-log-monitor": "https://github.com/alfiosalanitri/flask-log-monitor"
      }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
